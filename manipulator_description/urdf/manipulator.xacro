<link name="world"/>
<joint name="world_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/>  <!-- Place the robot wherever you want -->
    <parent link="world"/>
    <child link="base_link"/>  <!-- replace this name with your robot's base link -->
</joint>


<joint name="joint1" type="continuous">
  <origin xyz="0.0 0.0 0.04" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="link1_1"/>
  <axis xyz="0.0 0.0 1.0"/>
  <dynamics damping="1.0" friction="1.0"/>
</joint>

<joint name="joint2" type="continuous">
  <origin xyz="0.06 0.0 0.06" rpy="0 0 0"/>
  <parent link="link1_1"/>
  <child link="link2_1"/>
  <axis xyz="1.0 0.0 0.0"/>
  <dynamics damping="1.0" friction="1.0"/>
</joint>

<joint name="joint3" type="continuous">
  <origin xyz="0.0 0.0 0.35" rpy="0 0 0"/>
  <parent link="link2_1"/>
  <child link="link3_1"/>
  <axis xyz="-1.0 0.0 0.0"/>
  <dynamics damping="1.0" friction="1.0"/>
</joint>

<joint name="joint4" type="continuous">
  <origin xyz="0.005 0.0 0.31" rpy="0 0 0"/>
  <parent link="link3_1"/>
  <child link="link4_1"/>
  <axis xyz="1.0 0.0 0.0"/>
  <dynamics damping="1.0" friction="1.0"/>
</joint>

<joint name="joint5" type="continuous">
  <origin xyz="0.05 0.0 0.05" rpy="0 0 0"/>
  <parent link="link4_1"/>
  <child link="link5_1"/>
  <axis xyz="0.0 0.0 1.0"/>
  <dynamics damping="1.0" friction="1.0"/>
</joint>

<joint name="joint6" type="continuous">
  <origin xyz="0.05 0.0 0.05" rpy="0 0 0"/>
  <parent link="link5_1"/>
  <child link="end_effector_1"/>
  <axis xyz="1.0 0.0 0.0"/>
  <dynamics damping="1.0" friction="1.0"/>
</joint>


<xacro:macro name="control" params="joint">
    <transmission name="${joint}_transmission">
        <type>transmission_interface/SimpleTransmission</type>
        <!-- The EffortJointInterface works best with Gazebo using torques to actuate the joints -->
        <joint name="${joint}">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="${joint}_motor">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
</xacro:macro>

<xacro:control joint="joint1"/>
<xacro:control joint="joint2"/>
<xacro:control joint="joint3"/>
<xacro:control joint="joint4"/>
<xacro:control joint="joint5"/>
<xacro:control joint="joint6"/>



</robot>
